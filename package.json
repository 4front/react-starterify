{
  "name": "react-starterify",
  "version": "1.0.0",
  "description": "React JS starter template for 4front with all the goodies - ES6, babel, Gulp, auto-reload",
  "main": "app.js",
  "scripts": {
    "watch": "gulp watch",
    "build:debug": "gulp build",
    "build:release": "gulp build --release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4front/react-starterify"
  },
  "keywords": [
    "4front",
    "gulp",
    "react",
    "browserify",
    "seed",
    "boilerplate",
    "ES6",
    "babel"
  ],
  "author": "David Von Lehman",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/4front/react-starterify/issues"
  },
  "homepage": "https://github.com/4front/react-starterify",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "eslint": "^1.6.0",
    "eslint-config-4front": "4front/eslint-config-4front",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.8.11",
    "gulp-if": "^1.2.5",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.2.4",
    "merge": "^1.2.0",
    "reactify": "^1.1.0",
    "request": "^2.61.0",
    "rimraf": "^2.4.3",
    "run-sequence": "^1.1.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^2.4.0",
    "yargs": "^3.9.0"
  },
  "_virtualApp": {
    "baseDir": {
      "debug": "build/debug",
      "release": "build/release"
    },
    "autoReload": true,
    "router": [
      {
        "module": "webpage"
      }
    ]
  }
}
